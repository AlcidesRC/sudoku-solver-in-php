{
    "name": "alcidesrc/sudoku-solver-in-php",
    "type": "project",
    "license": "Unlicense",
    "description": "Another sudoku solver in PHP",
    "keywords": ["sudoku", "php"],
    "authors": [
        {
          "name": "Alcides Ramos",
          "email": "info@alcidesrc.com",
          "homepage": "http://alcidesrc.com"
        }
    ],
    "support": {
        "issues": "https://github.com/alcidesrc/sudoku-solver-in-php/issues",
        "source": "https://github.com/alcidesrc/sudoku-solver-in-php"
    },
    "require": {
        "php": "^8.0",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "require-dev": {
        "brianium/paratest": "^6.5",
        "codedungeon/phpunit-result-printer": "^0.32.0",
        "infection/infection": "^0.26.13",
        "nunomaduro/phpinsights": "^2.4",
        "php-parallel-lint/php-console-color": "^1.0",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmetrics/phpmetrics": "^2.8",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTests\\": "tests/Unit"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.7"
        },
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
